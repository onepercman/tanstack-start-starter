{
	"name": "tanstack-start-starter",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && tsc --noEmit",
		"serve": "vite preview",
		"test": "vitest run",
		"typecheck": "tsc",
		"lint": "biome lint .",
		"lint:fix": "biome lint --write .",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"check": "biome check . && pnpm typecheck",
		"fix": "biome check --write .",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{ts,tsx,js,jsx,json,jsonc}": [
			"biome check --write --no-errors-on-unmatched"
		]
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.1.17",
		"@tanstack/react-devtools": "^0.8.1",
		"@tanstack/react-query": "^5.90.10",
		"@tanstack/react-router": "^1.136.8",
		"@tanstack/react-router-devtools": "^1.136.8",
		"@tanstack/react-router-ssr-query": "^1.136.8",
		"@tanstack/react-start": "^1.136.8",
		"@vitejs/plugin-react": "^5.1.1",
		"axios": "^1.13.2",
		"clsx": "^2.1.1",
		"lucide-react": "^0.553.0",
		"next-themes": "^0.4.6",
		"nitro": "latest",
		"react": "^19.2.0",
		"react-aria-components": "^1.13.0",
		"react-dom": "^19.2.0",
		"recharts": "^3.4.1",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.1.17",
		"tailwindcss-react-aria-components": "^2.0.1",
		"tw-animate-css": "^1.4.0",
		"vite": "^7.2.2",
		"zustand": "^5.0.8"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.5",
		"@tanstack/devtools-vite": "^0.3.11",
		"@tanstack/router-plugin": "^1.136.8",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/react": "^16.3.0",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.5",
		"@types/react-dom": "^19.2.3",
		"husky": "^9.1.7",
		"jsdom": "^27.2.0",
		"lint-staged": "^16.2.6",
		"typescript": "^5.9.3",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^4.0.9",
		"web-vitals": "^5.1.0"
	}
}
